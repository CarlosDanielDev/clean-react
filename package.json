{
  "name": "clean-react",
  "version": "1.1.0",
  "description": "React App using TDD, Typescript and Clean Architecture",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "check": "npx npm-check -u -s"
  },
  "keywords": [
    "react",
    "clean code",
    "hooks",
    "clean architecture",
    "manguinho",
    "udemy"
  ],
  "author": "Carlos Daniel S. Oliviera",
  "license": "MIT",
